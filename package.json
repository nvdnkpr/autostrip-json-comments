  "dependencies": {
    "strip-json-comments": "~0.1.1"
  },
  "devDependencies": {
    "gt": "~0.8.20",
    "pre-git": "0.0.13",
    "matchdep": "~0.3.0",
    "grunt-contrib-jshint": "~0.7.2",
    "grunt": "~0.4.1",
    "grunt-readme": "~0.3.0",
    "time-grunt": "~0.2.1",
    "jshint-stylish": "~0.1.3",
    "grunt-nice-package": "~0.1.5"
  }
}
